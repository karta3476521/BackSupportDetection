        android:textSize="50sp"
        app:layout_constraintBottom_toBottomOf="@+id/rotateView9"
        app:layout_constraintEnd_toEndOf="@+id/rotateView9" />

    <com.boss.greenpower.backsupportdetection.measure.RotateView
        android:id="@+id/rotateView8"
        tools:visibility="visible"
        android:layout_width="0dp"
        android:layout_height="200dp"
        android:background="@drawable/custom_view_shape"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.boss.greenpower.backsupportdetection.measure.RotateView
        android:id="@+id/rotateView9"
        android:layout_width="0dp"
        android:layout_height="200dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/custom_view_shape"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rotateView8" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="yz"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/rotateView8"
        app:layout_constraintTop_toTopOf="@+id/rotateView8" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="yz"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/rotateView9"
        app:layout_constraintTop_toTopOf="@+id/rotateView9" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:text="分數"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/rotateView8"
        app:layout_constraintEnd_toStartOf="@+id/textView26" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:text="分數"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/rotateView9"
        app:layout_constraintEnd_toStartOf="@+id/textView27" />
</android.support.constraint.ConstraintLayout>